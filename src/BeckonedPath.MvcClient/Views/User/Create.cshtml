@model BeckonedPath.MvcClient.Models.Users



<form method="post" class="formpush userform">

    <div>
        <label asp-for="@Model.FirstMidName" class="control-label"></label>
        <input asp-for="@Model.FirstMidName" class="form-control" />
        <span asp-validation-for="@Model.FirstMidName" class="text-danger"></span>
    </div>
    <div>
        <label asp-for="@Model.LastName" class="control-label"></label>
        <input asp-for="@Model.LastName" class="form-control" />
        <span asp-validation-for="@Model.LastName" class="text-danger"></span>
    </div>

    <button type="submit" class="btn btn-large btn-success pull-right">Create</button>
</form>
