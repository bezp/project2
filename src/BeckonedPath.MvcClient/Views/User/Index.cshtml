
<table class="table">
    <thead>
        <tr>
            <th>
                First Name
            </th>
            <th>
                Last Name
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in ViewBag.wot)
        {
        <tr>
            <td>
                @item.FirstMidName
            </td>
            <td>
                @item.LastName
            </td>

            @if (ViewBag.wotId != null)
            {

                <td>
                </td>
            }
            else
            {
                <td>
                    <a asp-action="/index" asp-route-id="@item.UserId">View Info</a>

                </td>
            }


        </tr>
        }
    </tbody>
</table>


